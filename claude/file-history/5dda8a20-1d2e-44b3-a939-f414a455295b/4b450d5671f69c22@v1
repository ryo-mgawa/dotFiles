export type Input = {
  limit: number;
  offset: number;
};

export type GetShipmentItem = {
  id: string;
  slipId: string;
  consignor: { id: string; name?: string | null; };
  consignee: { id: string; name?: string | null; };
  originSpotId: string;
  destinationSpotId: string;
  meta?: object | null;
};

export type Output = {
  shipments: GetShipmentItem[];
};
