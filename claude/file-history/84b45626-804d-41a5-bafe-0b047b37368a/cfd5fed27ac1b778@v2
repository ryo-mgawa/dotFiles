description: シップメント追加成功レスポンス
type: object
required:
  - status
  - ids
properties:
  status:
    type: string
    description: 処理ステータス
    enum: [success]
    example: success
  ids:
    type: array
    description: 作成されたシップメントのIDリスト
    items:
      type: string
    example:
      - shipment-001
      - shipment-002
