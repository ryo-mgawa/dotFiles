import { ListGroup } from 'react-bootstrap';
import { Advisor } from '@/types/models/Advisor';

interface AdvisorListProps {
  advisors: Advisor[];
}

/**
 * 顧問一覧
 * requirements.md 2.3節を参照
 */
export const AdvisorList = ({ advisors }: AdvisorListProps) => {
  return (
    <ListGroup>
      {advisors.map((advisor) => (
        <ListGroup.Item key={advisor.id}>{advisor.name}</ListGroup.Item>
      ))}
    </ListGroup>
  );
};
