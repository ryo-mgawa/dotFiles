import { Inject, Injectable } from '@nestjs/common';
import { Response } from 'express';
import { AddShipmentEventsPresenterInterface } from '../../../interface/presenter/addShipmentEventsPresenterInterface';
import { Output } from '../dto/addShipmentEvents/output';

@Injectable()
export class AddShipmentEventsPresenter implements AddShipmentEventsPresenterInterface {
  constructor(@Inject('RESPONSE') private readonly response: Response) {}

  public output(): void {
    const result: Output = {
      status: 'success',
    };

    this.response.status(201).send(result);
  }
}
